<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.project.EditProjectController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="96.0" minHeight="10.0" prefHeight="85.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="43.0" minHeight="0.0" prefHeight="11.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Edit Project" GridPane.columnSpan="2" GridPane.halignment="CENTER">
            <font>
                <Font size="20.0" />
            </font>
        </Text>
        <Label text="Name" GridPane.rowIndex="1" />
        <Label text="Summary" GridPane.rowIndex="4" />
        <Label text="Type" GridPane.rowIndex="2" />
        <Label text="Deadline" GridPane.rowIndex="3" />
        <Text fx:id="message" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="279.0" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="6" />
        <Button mnemonicParsing="false" onAction="#handleReturn" text="Return" GridPane.halignment="CENTER" GridPane.rowIndex="7" />
        <Button defaultButton="true" mnemonicParsing="false" onAction="#handleConfirm" text="Confirm" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="7" />
        <TextArea fx:id="summaryField" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="5" />
        <TextField fx:id="nameField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <TextField fx:id="typeField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <DatePicker fx:id="deadLinePicker" prefHeight="25.0" prefWidth="359.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <Button mnemonicParsing="false" onAction="#handleDelete" style="-fx-background-color: red;" text="Delete Project" GridPane.columnIndex="1" GridPane.halignment="RIGHT" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</GridPane>
