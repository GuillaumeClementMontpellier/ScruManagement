<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import util.IconView?>
<GridPane hgap="10.0" prefWidth="590.0" style="-fx-background-color: #cccccc;" vgap="10.0"
          xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="gui.project.ProjectController">
    <columnConstraints>
        <ColumnConstraints maxWidth="262.0" minWidth="74.0" prefWidth="260.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="446.0" minWidth="10.0" prefWidth="260.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <AnchorPane>
            <children>
                <IconView fx:value="tasks" layoutX="7.5" layoutY="-27" onMouseClicked="#editProject"/>
                <Text fx:id="projectName" layoutX="40.0" layoutY="17.5" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="ProjectName"/>
            </children>
        </AnchorPane>
        <Button mnemonicParsing="false" onAction="#loadProject" prefWidth="130.0" style="-fx-background-color: lime;"
                text="Select Project" GridPane.columnIndex="1" GridPane.halignment="RIGHT"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</GridPane>
