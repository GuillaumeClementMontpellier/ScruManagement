        <ScrollPane prefHeight="200.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <GridPane fx:id="listPane" vgap="10.0">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
            </GridPane>
        </ScrollPane>
